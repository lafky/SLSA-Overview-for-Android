<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:ap="http://schemas.android.com/apk/res-auto"
    android:background="@color/colorPrimary"
    tools:context="com.example.tabstutorial.Fragment3">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="BORF:"
        android:id="@+id/textView19"
        android:textColor="@color/title_text"
        android:paddingLeft="20dp"
        android:layout_alignParentTop="true"
        android:layout_alignEnd="@+id/booH"
        android:layout_marginTop="31dp"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="BO/SR RF Phase Dif:"
        android:id="@+id/textView20"
        android:textColor="@color/title_text"
        android:paddingLeft="20dp"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView19"
        android:layout_alignParentLeft="true"
        android:layout_toStartOf="@+id/freqdif" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Ramping MPS:"
        android:id="@+id/textView21"
        android:textColor="@color/title_text"
        android:paddingLeft="20dp"
        android:layout_alignTop="@+id/rmps"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="                      "
        android:id="@+id/borf"
        android:textAlignment="textEnd"
        android:paddingRight="20dp"
        android:layout_above="@+id/textView20"
        android:layout_alignParentEnd="true"
        android:layout_toEndOf="@+id/textView24" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="                      "
        android:id="@+id/freqdif"
        android:layout_alignParentEnd="true"
        android:textAlignment="textEnd"
        android:paddingRight="20dp"
        android:layout_below="@+id/borf"
        android:layout_toEndOf="@+id/textView24"
        android:textColor="@color/neutral_PV" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="                       "
        android:id="@+id/rmps"
        android:textAlignment="textEnd"
        android:paddingRight="20dp"
        android:layout_below="@+id/freqdif"
        android:layout_alignParentEnd="true"
        android:layout_toEndOf="@+id/textView24" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="---------Booster Magnets---------"
        android:id="@+id/textView22"
        android:textAlignment="center"
        android:paddingBottom="10dp"
        android:textColor="@color/title_text"
        android:layout_marginTop="21dp"
        android:layout_below="@+id/textView21"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="-----------BTS Magnets-----------"
        android:id="@+id/textView24"
        android:textAlignment="center"
        android:paddingBottom="10dp"
        android:textColor="@color/title_text"
        android:layout_alignStart="@+id/textView22"
        android:layout_below="@+id/booS" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="    S    "
        android:id="@+id/booS"
        android:layout_below="@+id/textView22"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="    Q    "
        android:id="@+id/booQ"
        android:layout_below="@+id/textView22"
        android:layout_toStartOf="@+id/booS" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="    V    "
        android:id="@+id/booV"
        android:layout_below="@+id/textView22"
        android:layout_toEndOf="@+id/booH" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="    H    "
        android:id="@+id/booH"
        android:layout_below="@+id/textView22"
        android:layout_toEndOf="@+id/booS" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="    D    "
        android:id="@+id/booD"
        android:layout_above="@+id/textView24"
        android:layout_toStartOf="@+id/booQ" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="    D    "
        android:id="@+id/btsD"
        android:layout_below="@+id/textView24"
        android:layout_toStartOf="@+id/btsQ" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="    Q    "
        android:id="@+id/btsQ"
        android:layout_below="@+id/textView24"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="    V    "
        android:id="@+id/btsV"
        android:layout_below="@+id/textView24"
        android:layout_toEndOf="@+id/btsQ" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="--------RF and Ramping Magnets--------"
        android:id="@+id/textView26"
        android:textColor="@color/title_text"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <com.androidplot.xy.XYPlot
        style="@style/APDefacto.Dark"
        android:id="@+id/plot"
        ap:label="BORF:"
        android:layout_centerHorizontal="true"
        android:layout_width="match_parent"
        android:layout_alignParentBottom="true"
        ap:labelTextColor="@color/title_text"
        ap:paddingBottom="-30dp"
        ap:paddingLeft="-25dp"
        ap:paddingRight="-15dp"
        ap:paddingTop="0dp"
        android:layout_height="200dp" />

</RelativeLayout>